application: syndicate-metadata # <--- change this to your application's name on the platform
version: 1
runtime: python27
api_version: 1
threadsafe: yes

inbound_services:
- warmup

builtins:
- appstats: on
- admin_redirect: on
- deferred: on

handlers:
- url: /cron.*
  script: cron.app
  login: admin

- url: /CERT.*
  script: msapp.app
  secure: optional

- url: .*
  script: msapp.app
  secure: always

libraries:
- name: webapp2
  version: "2.5.2"
- name: lxml
  version: latest
- name: pycrypto
  version: latest
- name: django
  version: 1.4

